{
  "name": "eslint-plugin-no-package-self-import",
  "version": "0.1.0",
  "description": "An ESLint plugin for finding instances of a package importing from itself",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viodotcom/eslint-plugin-no-package-self-import.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Gerrit Burger",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-n": "^17.0.0",
    "npm-run-all2": "^6.1.2"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "license": "MIT"
}
